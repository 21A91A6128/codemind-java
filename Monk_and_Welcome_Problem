import java.util.*;
class mawp{
    public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt(),i=0,j=0;
        int [] arr=new int [n];
        for (i=0;i<n;i++)
            arr[i]=sc.nextInt();
        for (i=0;i<n;i++){
            arr[i]+=sc.nextInt();
            System.out.print(arr[j]+" ");
            j++;
        }
    }
}
