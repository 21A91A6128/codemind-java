import java.util.*;
class majele{
    public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        ArrayList<Integer> ar=new ArrayList<Integer>();
        for (int i=0;i<n;i++)
            ar.add(sc.nextInt());
        for (int ele:ar)
            if (Collections.frequency(ar,ele)>(int)n/2)
            {
                System.out.print(ele);
                break;
            }
    }
}
