import java.util.*;
class ea{
    public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int vk=0,c=0;
        for (int i=0;i<n;i++)
        {
            vk=sc.nextInt();
            if (vk%2==0)
                c++;
        }
        if(c==n){
            System.out.println("True");
        }
        else{
            System.out.println("False");
        }
    }
}
