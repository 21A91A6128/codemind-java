import java.util.*;
class numeve{
    public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
        int n1=sc.nextInt();
        int vk=0;
        int [] arr1=new int [n1];
        for (int i=0;i<n1;i++)
            arr1[i]=sc.nextInt();
        for (int i=0;i<n1;i++)
            if (((int)Math.log10(arr1[i])+1)%2==0){
                vk++;
            }
        System.out.print(vk);
    }
}
